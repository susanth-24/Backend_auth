<!DOCTYPE html>
<html>
<head>
	<title>Welcome</title>
	
	<link rel="stylesheet" href="./css/main.css">
</head>
<body>
	<div class="col-md-4 col-md-offset-4">
		<div>
			<p>Profile Detail</p>
		</div>
		<div>
			<center>
				<table class="table table-hover">
					<tr><td>Name </td><td><%= name %></td></tr>
					<tr><td>Email </td><td><%= email %></td></tr>
				</table>
			</center>
		</div>
		<div class="abc">
			<a href="/logout">Logout</a>
		</div>
	</div>
    <div2 style="margin-top:2cm">
        <form class="" action="/profile" method="post">
            <div class="form-group">
            <label>Tag</label>
            <input class="form-control" type="text" name="newtitle" >
            <label>Link</label>
            <input class="form-control" type="text" name="newlink" >
        </div>
            <button type="submit" class="btn btn-primary" name="button">publish</button>
        
        </form>
    </div2>
	<% d.forEach(function(datsa){%>
        
        <h1><%=datsa.tag%></h1>
        <h1><%=datsa.link%></h1>

    <%})%>
	
</body>
</html>